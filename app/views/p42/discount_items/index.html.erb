<h1>Listing p42_discount_items</h1>

<table>
  <thead>
    <tr>
      <th>Auto apply</th>
      <th>Discount amount</th>
      <th>Discount item</th>
      <th>Menu item</th>
      <th>Pos ticket</th>
      <th>Pos ticket item</th>
      <th>Reason text</th>
      <th>Ticket</th>
      <th>Ticket item</th>
      <th>Ticket item price</th>
      <th>When</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @p42_discount_items.each do |p42_discount_item| %>
      <tr>
        <td><%= p42_discount_item.auto_apply %></td>
        <td><%= p42_discount_item.discount_amount %></td>
        <td><%= p42_discount_item.discount_item_id %></td>
        <td><%= p42_discount_item.menu_item_id %></td>
        <td><%= p42_discount_item.pos_ticket_id %></td>
        <td><%= p42_discount_item.pos_ticket_item_id %></td>
        <td><%= p42_discount_item.reason_text %></td>
        <td><%= p42_discount_item.ticket_id %></td>
        <td><%= p42_discount_item.ticket_item_id %></td>
        <td><%= p42_discount_item.ticket_item_price %></td>
        <td><%= p42_discount_item.when %></td>
        <td><%= link_to 'Show', p42_discount_item %></td>
        <td><%= link_to 'Edit', edit_p42_discount_item_path(p42_discount_item) %></td>
        <td><%= link_to 'Destroy', p42_discount_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Discount item', new_p42_discount_item_path %>
