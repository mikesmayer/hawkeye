<h1>Listing p42_meal_count_rules</h1>

<table>
  <thead>
    <tr>
      <th>P42 menu item</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Meal modifier</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @p42_meal_count_rules.each do |p42_meal_count_rule| %>
      <tr>
        <td><%= p42_meal_count_rule.menu_item_id %></td>
        <td><%= p42_meal_count_rule.start_date %></td>
        <td><%= p42_meal_count_rule.end_date %></td>
        <td><%= p42_meal_count_rule.meal_modifier %></td>
        <td><%= link_to 'Show', p42_meal_count_rule %></td>
        <td><%= link_to 'Edit', edit_p42_meal_count_rule_path(p42_meal_count_rule) %></td>
        <td><%= link_to 'Destroy', p42_meal_count_rule, method: :delete, disable_with: "Deleting...", data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

