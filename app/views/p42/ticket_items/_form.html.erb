<%= simple_form_for(@p42_ticket_item) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :ticket_item_id %>
    <%= f.input :ticket_id %>
    <%= f.input :menu_item_id %>
    <%= f.input :category_id %>
    <%= f.input :revenue_class_id %>
    <%= f.input :customer_original_id %>
    <%= f.input :quantity %>
    <%= f.input :net_price %>
    <%= f.input :discount_total %>
    <%= f.input :item_menu_price %>
    <%= f.input :choice_additions_total %>
    <%= f.input :ticket_close_time %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
