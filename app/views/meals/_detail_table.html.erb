<table class="table table-striped table-hover datatable" id="m4m_stats_tbl">
	<thead>
		<tr>
			<th>Date</th>
			<th>Meal 4 Meal</th>
			<th>Double</th>
			<th>Apparel</th>
			<th>Tip Jar</th>
			<th>Total</th>
		</tr>
	</thead>
	<tbody>
		<% @detail_totals.each do |row| %>
			<tr>
				<td><%= row.date %></td>
				<td><%= sprintf('%.0f', row.m4m) %></td>
				<td><%= sprintf('%.0f', row.dym) %></td>
				<td><%= sprintf('%.0f', row.apparel) %></td>
				<td><%= number_with_precision(row.tip_jar, :precision => 0, :delimiter => ",") %></td>
				<td><%= sprintf('%.0f', row.total) %></td>
			</tr>
		<% end %>
	</tbody>
</table>