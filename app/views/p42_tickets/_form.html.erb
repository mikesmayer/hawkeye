<%= simple_form_for(@p42_ticket) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :pos_ticket_id %>
    <%= f.input :gross_price %>
    <%= f.input :net_price %>
    <%= f.input :meal_for_meal %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
